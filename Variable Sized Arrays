#include <iostream> 
#include <fstream>
#include <algorithm>
#include <queue>
#include <stack>
#include <map>
#include <set>
#include <list>
#include <cstdio>
#include <cstring>
#include <string>
#include <cmath>
#include <cstdlib>
#include <vector>
#include <climits>
using namespace std;
vector<vector<int>> myvector;
int main() {
    ios_base::sync_with_stdio(false);
cin.tie(NULL);
    int n,Q,temp,a,qu_,QU; cin>>n>>Q;
    myvector.assign(n, vector<int>());
    for(int j=0;j<n;j++){
    cin>>a;
        for(int i=0;i<a;i++) { cin>>temp; myvector[j].push_back(temp); }
    }
    
    while(Q--) { cin>>qu_>>QU;  cout<<myvector[qu_][QU]<<endl; }
    return 1;
    return 0;
}
