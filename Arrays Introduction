#include <map>
#include <set>
#include <list>
#include <cstdio>
#include <cstring>
#include <string>
#include <cmath>
#include <cstdlib>
#include <vector>
#include <climits>
using namespace std;
int main() {
    int n,a[100000]; cin>>n;
    for(int i=0;i<n;i++) cin>>a[i];
    for(int i=n-1;i>-1;i--) cout<<a[i]<<" ";
    return 0;
}
